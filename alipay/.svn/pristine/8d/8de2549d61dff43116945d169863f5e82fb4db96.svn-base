img {
	border: currentColor; border-image: none;
}
.navContainer {
	margin: auto; font-family: Arial, sans-serif; font-size: 1.1em;
}
.navTopRow {
	padding-bottom: 3px; vertical-align: bottom;
}
.navLogo {
	float: left;
}
.navWw {
	padding-right: 0px; margin-top: 29px; float: right;
}
.navWw a {
	color: rgb(1, 123, 186); font-size: smaller; text-decoration: none; vertical-align: top;
}
.navNw a:hover {
	text-decoration: underline;
}
.navBottomRow {
	margin-left: 0px;
}
.navBottomRowLinks div {
	padding: 5px 0px; width: 163px; text-align: center; float: left;
}
.navBottomRowLinks div:hover {
	cursor: pointer; background-color: rgb(239, 239, 239);
}
.navBottomRowLinks a {
	color: rgb(81, 98, 111); font-weight: bold; text-decoration: none;
}
.navGraphic {
	margin-top: 5px;
}
.navBorderLeft {
	border-left-color: rgb(196, 207, 209); border-left-width: 1px; border-left-style: solid;
}
.navBorderRight {
	border-right-color: rgb(196, 207, 209); border-right-width: 1px; border-right-style: solid;
}
.navBorderTop {
	border-top-color: rgb(196, 207, 209); border-top-width: 1px; border-top-style: solid;
}
.navBorderBottom {
	border-bottom-color: rgb(196, 207, 209); border-bottom-width: 1px; border-bottom-style: solid;
}
img {
	margin: 0px; padding: 0px; border: currentColor; border-image: none;
}
.footer {
	margin: 10px 0px; border: 1px solid rgb(196, 207, 209); border-image: none; height: 23px; line-height: 23px; font-size: smaller;
}
.footer a {
	color: rgb(1, 123, 186); padding-left: 1px; text-decoration: none;
}
.footer a:hover {
	text-decoration: underline;
}
.footer img {
	vertical-align: middle;
}
.copyright {
	margin-right: 4px; float: right;
}
